/*!
 * \file imagergb.cpp
 */

#include <imageplus/core/imagergb.hpp>

namespace imageplus
{
    template<typename T>
    ImageRGB<T>::ImageRGB()
    :   Base()
    {}

    template<typename T>
    ImageRGB<T>::ImageRGB(const ImaVol<T,3,2>& copy)
    :   Base(copy)
    {}

    template<typename T>
    ImageRGB<T>::ImageRGB(std::size_t width, std::size_t height)
    :   Base(width,height)
    {}

    template<typename T>
    ImageRGB<T>::ImageRGB(const std::vector< std::vector<uint64> >& dims)
    :   Base(dims)
    {}

    template<typename T>
    const ImageRGB<T>& ImageRGB<T>::operator=( const ImaVol<T,3,2>& copy )
    {
        Base::operator=(copy);
        return *this;
    }

    template<typename T>
    const ImageRGB<T>& ImageRGB<T>::operator=( const T& scalar )
    {
        Base::operator=(scalar);
        return *this;
    }
} // namespace imageplus


/*
 * Template instantiations
 */
namespace imageplus
{
    template class ImageRGB<uint8>;
    template class ImageRGB<uint16>;
    template class ImageRGB<uint32>;
    template class ImageRGB<uint64>;

    template class ImageRGB<int8>;
    template class ImageRGB<int16>;
    template class ImageRGB<int32>;
    template class ImageRGB<int64>;

    template class ImageRGB<float64>;
}
