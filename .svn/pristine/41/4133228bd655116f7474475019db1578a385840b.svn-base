#include <sstream>
#include <string>

#include <imageplus/semantic/instance.hpp>

using namespace imageplus::semantic;

const char *Instance::instance_type_names[] = {"positive", "negative", "neutral"};

std::string imageplus::semantic::Instance::get_media_uri() const
{
	return _media_uri;
}

std::ostream& imageplus::semantic::operator<<(std::ostream &os, const Instance& i)
{
	return os << "(" << i._image_id << ") " << i._media_uri;
}

