#define ABS(x) ((x) > 0 ? (x) : -(x))
#define MIN(a,b) ((a) < (b) ? (a) : (b))


float64 imageplus::math::numeric::n_choose_r (int64 n, int64 r)
{
  if (n < r)
    {
      int64 temp;
      temp = n;
      n = r;
      r = temp;
    }
  r = MIN (ABS (n - r), r);
  return PascalsTriangle[n][r];
}

